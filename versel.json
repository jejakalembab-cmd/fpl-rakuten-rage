{
  "version": 2,
  "builds": [
    {
      "src": "api/team.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "fpl-logo.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/team",
      "dest": "api/team.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}